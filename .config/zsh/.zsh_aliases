alias la='LC_COLLATE=C ls -lAFh --color=auto'   #long list,show almost all,show type,human readable
alias lt='ls -ltFh --color=auto'   #long list,sorted by date,show type,human readable
alias ll='ls -l --color=auto'      #long list
alias ldot='ls -ld .*'
alias zshrc="vim ~/.config/zsh/.zshrc"
alias vrc="vim ~/.config/nvim/init.vim"
alias dotgit="git --git-dir=$HOME/.dotfiles/ --work-tree=$HOME"
alias yt="ytfzf -t"
